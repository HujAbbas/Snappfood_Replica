        <ImageView fitHeight="34.0" fitWidth="33.0" layoutX="184.0" layoutY="327.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../images/user.png" />
            </image>
        </ImageView>
        <TextField layoutX="224.0" layoutY="393.0" prefHeight="33.0" prefWidth="164.0" promptText="Enter your new password" />
        <TextField layoutX="224.0" layoutY="327.0" prefHeight="33.0" prefWidth="164.0" promptText="Choose a username" />
        <ImageView fitHeight="34.0" fitWidth="33.0" layoutX="184.0" layoutY="393.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../images/password.png" />
            </image>
        </ImageView>
        <Label fx:id="snappFoodLabel" layoutX="240.0" layoutY="46.0" stylesheets="small-snapp-food-label.css" text="Snapp! Food" />
        <Label fx:id="usernameErrorLabel" layoutX="225.0" layoutY="368.0" prefHeight="18.0" prefWidth="152.0" text="خطا" />
